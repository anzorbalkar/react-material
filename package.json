{
  "name": "react-material",
  "version": "0.0.1",
  "description": "React implementation of Google Material Design UI components",
  "main": "./build",
  "scripts": {
    "test": "echo \"No tests\" && exit 1",
    "prebuild": "rm -rf ./build && rm -rf ./dist",
    "prebuild:dev": "rm -rf ./build",
    "prebuild:prod": "rm -rf ./dist",
    "build:dev": "webpack --progress --profile --colors",
    "build:prod": "NODE_ENV=production webpack -p",
    "build": "npm run build:dev && npm run build:prod",
    "prepublish": "npm run build"
  },
  "author": "Anzor Balkar <abalkar@gmail.com>",
  "keywords": [
    "react",
    "react-component",
    "material design",
    "material-ui",
    "material ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anzorbalkar/react-material.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anzorbalkar/react-material/issues"
  },
  "devDependencies": {
    "lodash": "",
    "react": "0.13.1",
    "jsx-loader": "^0.12.2",
    "extract-text-webpack-plugin": "",
    "file-loader": "",
    "url-loader": "",
    "css-loader": "",
    "less-loader": "",
    "less": "^2.3.1",
    "webpack": "^1.4.13"
  },
